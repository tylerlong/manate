{
  "name": "manate",
  "version": "2.0.3",
  "repository": "git@github.com:tylerlong/manate.git",
  "license": "MIT",
  "types": "dist/esm/index.d.ts",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./react": {
      "types": "./dist/esm/react/index.d.ts",
      "import": "./dist/esm/react/index.js",
      "require": "./dist/cjs/react/index.js"
    },
    "./react/class-components": {
      "types": "./dist/esm/react/class-components.d.ts",
      "import": "./dist/esm/react/class-components.js",
      "require": "./dist/cjs/react/class-components.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json && echo '{\"type\":\"module\"}' > dist/esm/package.json && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "prepublishOnly": "yarn build",
    "postpublish": "rm -rf dist",
    "lint": "deno fmt && deno lint --fix",
    "test": "yarn vitest run"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "jsdom": "^27.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "shell-commands": "^0.4.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8",
    "wait-for-async": "^0.8.0",
    "yarn-upgrade-all": "^0.7.5"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
